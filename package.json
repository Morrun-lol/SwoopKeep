{
  "name": "trae-project",
  "private": true,
  "version": "0.0.0",
  "main": "./out/main/index.js",
  "author": "Trae",
  "description": "Voice Expense Tracker",
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "lint": "eslint .",
    "check": "tsc -b --noEmit",
    "dist": "npm run build && electron-builder",
    "build:web": "vite build -c vite.config.web.ts",
    "dev:web": "vite -c vite.config.web.ts",
    "dev:api": "node api/server.js"
  },
  "build": {
    "appId": "com.trae.expense.tracker",
    "productName": "SwoopKeep",
    "directories": {
      "output": "dist"
    },
    "asar": false,
    "files": [
      "out/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    }
  },
  "dependencies": {
    "@capacitor-community/speech-recognition": "^7.0.1",
    "@capacitor/app": "^8.0.0",
    "@capacitor/core": "^8.0.1",
    "@capacitor/haptics": "^8.0.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.2.2",
    "@supabase/supabase-js": "^2.90.1",
    "@types/crypto-js": "^4.2.2",
    "@types/ws": "^8.18.1",
    "better-sqlite3": "^12.6.0",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jimp": "^1.6.0",
    "lucide-react": "^0.511.0",
    "node-xlsx": "^0.24.0",
    "openai": "^6.16.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.71.1",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "tesseract.js": "^7.0.0",
    "undici": "^7.18.2",
    "ws": "^8.19.0",
    "xlsx": "^0.20.2",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@capacitor/cli": "^8.0.1",
    "@capacitor/ios": "^8.0.1",
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.19.7",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
